{
    "name": "teknoo/gd-text",
    "description": "A class drawing multiline and aligned text on pictures. Uses GD extension.",
    "homepage": "https://teknoo.software/libraries/gd-text",
    "license": ["BSD-3-Clause"],
    "type": "library",
    "keywords": [
        "gd",
        "text",
        "image",
        "draw"
    ],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richard@teknoo.software",
            "role": "Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/gd-text/issues",
        "source": "https://github.com/TeknooSoftware/gd-text"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "config": {
        "audit": {
            "abandoned": "report"
        },
        "optimize-autoloader": true
    },
    "require": {
        "php": "^8.4",
        "ext-gd": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^13",
        "squizlabs/php_codesniffer": "^4.0.1",
        "phpstan/phpstan": "^2.1.38",
        "symfony/property-access": "^6.4||^7.4||^8"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GDText\\": "src/",
            "GDText\\Tests\\": "tests/"
        }
    },
    "replace": {
        "pe46dro/gd-text": "*"
    }
}
