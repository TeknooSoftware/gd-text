{
    "name": "teknoo/gd-text",
    "description": "A class drawing multiline and aligned text on pictures. Uses GD extension.",
    "homepage": "http://teknoo.software/gd-text",
    "license": ["MIT"],
    "type": "library",
    "keywords": [
        "gd",
        "text",
        "image",
        "draw"
    ],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/gd-text/issues",
        "source": "https://github.com/TeknooSoftware/gd-text"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": "^8.1",
        "ext-gd": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5.27",
        "squizlabs/php_codesniffer": "^3.7.1",
        "sebastian/phpcpd": "^6.0.3",
        "phploc/phploc": "^7.0.2",
        "phpstan/phpstan": "^1.9.3",
        "symfony/property-access": "^6.2",
        "icanhazstring/composer-unused": "^0.8.5",
        "nikic/php-parser": "^v4.15.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GDText\\": "src/",
            "GDText\\Tests\\": "tests/"
        }
    },
    "replace": {
        "pe46dro/gd-text": "*"
    }
}
